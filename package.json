{
  "name": "treasure-form",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public -b $(cat .env) --kv ATLAS_MINE_HARVESTER_PARTS",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "lint": "prettier --write 'app/**/{*.ts,*.tsx}' && eslint --fix --no-error-on-unmatched-pattern 'app/**/{*.ts,*.tsx}'",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@rainbow-me/rainbowkit": "^0.5.0",
    "@remix-run/cloudflare": "^1.7.0",
    "@remix-run/cloudflare-pages": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "buffer": "^6.0.3",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "ethers": "^5.7.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0",
    "wagmi": "^0.6.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/forms": "^0.5.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "16.7.0"
  },
  "lint-staged": {
    "app/**/{*.ts,*.tsx}": "npm run lint"
  }
}
